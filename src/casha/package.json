{
  "name": "casha",
  "main": "expo-router/entry",
  "version": "0.0.1",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "android-device": "expo run:android --device",
    "ios-device": "expo run:ios --device",
    "test:dev": "jest -o --watch --coverage=false --passWithNoTests",
    "test": "jest --passWithNoTests",
    "lint": "expo lint",
    "pod-install": "pod-install",
    "android-build": "react-native build-android --mode=release",
    "ios-build": "react-native build-ios --mode=Release",
